  "dependencies": {
    "google-libphonenumber": "^3.2.32",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "@react-native-picker/picker": ">= 2.0",
    "react-native": ">=0.69.9"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^5.2.7",
    "@types/react-native": "^0.63.69",
    "@types/react-native-phone-input": "^0.2.2",
