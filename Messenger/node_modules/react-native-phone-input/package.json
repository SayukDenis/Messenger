    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^4.3.8",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.14.2",
    "typescript": "^4.9.5"
