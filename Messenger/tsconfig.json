{
  "typeRoots": [
    "node_modules/@types",
    "node_modules"
  ],
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es2017",
    "strict": true,
    "module": "esnext",
    "jsx": "react",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
  },
  "paths": {
    "expo": [
      "node_modules/expo"
    ]
  }
}